<%- include("./header.ejs") %>
  <h1 class="my-4 text-center text-lg-left">
    <%= title %>
  </h1>

  <div class="row text-center text-lg-left">

    <% data.forEach(function(item,index){%>
      <div class="col-lg-3 col-md-4 col-xs-6">
        <a href="/gifmaker?id=<%= index %>" class="d-block mb-4 h-100">
          <img class="img-fluid img-thumbnail" src="<%= item.thumbnail %>" alt="">
        </a>
      </div>
      <% }) %>

  </div>

  <%- include("./footer.ejs") %>